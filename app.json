{
  "expo": {
    "name": "housekeeping_app",
    "slug": "housekeeping_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/housekeeping_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/housekeeping_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.roloct.housekeeping_app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/housekeeping_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "412f1ff6-a0e0-4a73-bbb4-44fdde11643f"
      }
    }
  }
}
